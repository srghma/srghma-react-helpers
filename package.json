{
  "name": "srghma-react-helpers",
  "version": "0.0.5",
  "description": "",
  "author": "Sergey Homa <srghma@gmail.com>",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "browser": "umd/index.js",
  "module": "es/index.js",
  "scripts": {
    "prepare": "install-self-peers -- --ignore-scripts",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "precommit": "lint-staged",
    "build": "npm run build:es && npm run build:commonjs",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "clean": "rimraf es/ lib/ umd/",
    "lint": "eslint --ignore-path .eslintignore .",
    "lint:fix": "eslint --ignore-path .eslintignore --fix .",
    "format:base": "prettier --config './.prettierrc.yaml' --ignore-path './.prettierignore' --write",
    "format": "npm run format:base './**/*.{js,json}'",
    "format-fix": "npm run format && npm run lint:fix",
    "test": "NODE_ENV=test jest"
  },
  "lint-staged": {
    "*.js": [
      "npm run format:base",
      "eslint --ignore-path .eslintignore --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/helper-annotate-as-pure": "^7.0.0-beta.46",
    "@babel/plugin-external-helpers": "^7.0.0-beta.46",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@team-griffin/install-self-peers": "^1.1.1",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "prettier": "^1.9.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^4.0.0-beta.4"
  },
  "peerDependencies": {
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.1.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2"
  },
  "dependencies": {}
}
